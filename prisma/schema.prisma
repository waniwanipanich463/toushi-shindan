generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Result {
  id        String   @id @default(uuid())
  s_g       Int      // Safe vs Growth (-scores to +scores)
  i_a       Int      // Index vs Active
  l_t       Int      // Long vs Short
  d_f       Int      // Diversified vs Focused
  type      String   // e.g., "SILD"
  createdAt DateTime @default(now())

  // Optional: UserId if we implement auth later
  userId    String?
}
